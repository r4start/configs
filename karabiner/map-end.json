{
    "description": "Map End to Cmd + Right Arrow",
    "manipulators": [
        {
            "conditions": [
                {
                    "bundle_identifiers": [
                        "^com\\.electron\\.logseq$",
                        "^com\\.sublimetext\\.4$"
                    ],
                    "type": "frontmost_application_if"
                }
            ],
            "from": { "key_code": "end" },
            "to": [
                {
                    "key_code": "right_arrow",
                    "modifiers": ["left_command"]
                }
            ],
            "type": "basic"
        }
    ]
}
