{
    "description": "Map Home to Cmd + Left Arrow",
    "manipulators": [
        {
            "conditions": [
                {
                    "bundle_identifiers": [
                        "^com\\.electron\\.logseq$",
                        "^com\\.sublimetext\\.4$"
                    ],
                    "type": "frontmost_application_if"
                }
            ],
            "from": { "key_code": "home" },
            "to": [
                {
                    "key_code": "left_arrow",
                    "modifiers": ["left_command"]
                }
            ],
            "type": "basic"
        }
    ]
}
